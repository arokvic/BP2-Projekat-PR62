<Window x:Class="UI.AddWindows.MastercardAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UI.AddWindows"
        mc:Ignorable="d"
     Title="MastercardAdd" Height="334.426" Width="445.492">
    <Grid Margin="0,0,2,2">
        <ComboBox Name="cmbC" Margin="227,121,88,154" RenderTransformOrigin="0.495,-0.43" ItemsSource="{Binding Cnames}"/>
        <Button x:Name="addBtn" Content="Add" HorizontalAlignment="Left" Margin="174,207,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-1.082,-7.48" Click="AddBtn_Click"/>
        <Label Content="Account number" HorizontalAlignment="Left" Margin="103,75,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.526,-1.47"/>
        <Label Content="Customer" HorizontalAlignment="Left" Margin="103,121,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.357,2.122"/>
        <TextBox x:Name="numberTxt" HorizontalAlignment="Left" Height="23" Margin="227,78,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" RenderTransformOrigin="-0.531,0.517"/>

    </Grid>
</Window>
